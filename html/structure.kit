<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>EUBC - Europen Boxing Confederation</title>
    <!-- @include styles -->
</head>

<body>
    <!-- @include nav -->
    <!-- @include header -->
    <main class="main">
        <section id="structure" class="section-padding structure">
            <div class="container">
                <div class="section-titlebox">
                    <h2 class="section-title section-title--first">structure</h2>
                    <h3 class="section-subtitle section-subtitle--first">directors</h3>
                    <span class="section-underline"></span>
                </div>
                <p class="structure__title">Board of Directors of European Boxing Confederation</p>
                <div class="structure__boxes">
                    <div class="structure__box structure__box--img">
                        <p class="structure__box-text">EUBC President
                            Dr. Ioannis Filippatos (Greece)</p>
                        <img src="./dist/img/eubcpresident.jpg" alt="EUBC CEO, EUBC President, Dr. Ioannis Filippatos"
                            class="structure__img">
                    </div>
                    <div class="structure__box structure__box--table">
                        <table class="structure__box-table">
                            <tbody class="structure__box-tbody">
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th structure__box-th--fr">Name</th>
                                    <th class="structure__box-th structure__box-th--fr">Role</th>
                                    <th class="structure__box-th structure__box-th--fr">Country</th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Sumaid Khalidov</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="RU"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Krasimir Ininski</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="BG"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Felipe Jos√® Martinez Martinez</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="ES"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Nenad Borovcanin</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="RS"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Vasile Citea</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="RO"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Ohanes Ovsepian</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="AM"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Alexander Klemenko</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="ME"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Andre Micallef</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="MC"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Zsuzsanna Toth</th>
                                    <th class="structure__box-th">Director</th>
                                    <th class="structure__box-th" data-flag="HU"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Irma Testa</th>
                                    <th class="structure__box-th">Athletes Committee
                                    </th>
                                    <th class="structure__box-th" data-flag="IT"></th>
                                </tr>
                                <tr class="structure__box-tr">
                                    <th class="structure__box-th">Dmytro Tonyshev</th>
                                    <th class="structure__box-th">Athletes Committee
                                    </th>
                                    <th class="structure__box-th" data-flag="IT"></th>
                                </tr>
                            </tbody>
                        </table>

                    </div>
                </div>
            </div>
        </section>
        <section class="section section-padding socials socials-white">
            <div class="container">
                <div class="section-titlebox titlebox--left">
                    <h2 class="section-title section-title--second">social channels</h2>
                    <h3 class="section-subtitle section-subtitle--second">socials</h3>
                    <span class="section-underline"></span>
                </div>
                <div class="socials__boxes">
                    <div class="socials__box socials__box-facebook card--navy">
                        <div class="socials__box-top">
                            <ion-icon name="logo-facebook" class="socials__box-icon socials__box-icon--facebook">
                            </ion-icon>
                            <p class="socials__box-title card-title--white">Facebook</p>
                        </div>
                        <div class="socials__box-middle">
                            <p class="socials__box-description  card-description--white">Check out our Facebook
                                channel to stay with us up to
                                date</p>
                        </div>
                        <a href="https://www.facebook.com/EuropeanBoxingConfederation"
                            class="socials__box-btn card-btn--white">read
                            more</a>
                    </div>
                    <div class="socials__box socials__box-instagram card--navy">
                        <div class="socials__box-top">
                            <ion-icon name="logo-instagram"
                                class="socials__box-icon socials__box-icon--instagram socials__box-icon--instagram-white">
                            </ion-icon>
                            <p class="socials__box-title card-title--white">Instagram</p>
                        </div>
                        <div class="socials__box-middle">
                            <p class="socials__box-description card-description--white">Check out our Instagram
                                channel to stay with us up to
                                date</p>
                        </div>
                        <a href="https://www.instagram.com/eubcboxing/" class="socials__box-btn card-btn--white">read
                            more</a>
                    </div>
                    <div class="socials__box socials__box-tiktok card--navy">
                        <div class="socials__box-top">
                            <ion-icon name="logo-tiktok" class="socials__box-icon socials__box-icon--tiktok">
                            </ion-icon>
                            <p class="socials__box-title card-title--white">TikTok</p>
                        </div>
                        <div class="socials__box-middle">
                            <p class="socials__box-description card-description--white">Check out our TikTok
                                channel to stay with us up to
                                date</p>
                        </div>
                        <a href="#" class="socials__box-btn card-btn--white">read more</a>
                    </div>
                    <div class="socials__box socials__box-youtube card--navy">
                        <div class="socials__box-top">
                            <ion-icon name="logo-youtube" class="socials__box-icon socials__box-icon--youtube">
                            </ion-icon>
                            <p class="socials__box-title card-title--white">YouTube</p>
                        </div>
                        <div class="socials__box-middle">
                            <p class="socials__box-description card-description--white">Check out our YouTube
                                channel to stay with us up to
                                date</p>
                        </div>
                        <a href="#" class="socials__box-btn card-btn--white">read more</a>
                    </div>
                </div>
            </div>
        </section>
        <!-- @include footer-->

    </main>
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    <script src="./dist/js/main.min.js"></script>
</body>

</html>